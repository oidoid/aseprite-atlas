{
  "compilerOptions": {
    "exactOptionalPropertyTypes": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "useUnknownInCatchVariables": true,
    "lib": ["dom", "deno.ns"]
  },
  "fmt": {
    "files": { "exclude": ["dist"] },
    "options": { "semiColons": false, "singleQuote": true }
  },
  "imports": {
    "@/atlas-pack": "./mod.ts",
    "@/ooz": "https://deno.land/x/ooz@v0.0.16/mod.ts",
    "std/": "https://deno.land/std@0.179.0/"
  },
  "lint": {
    "files": { "exclude": ["dist"] },
    "rules": {
      "exclude": [
        "no-empty-interface",
        "no-inferrable-types",
        "no-misused-new",
        "no-namespace"
      ]
    }
  },
  "lock": false,
  "name": "atlas-pack",
  "test": { "files": { "exclude": ["dist"] } },
  "version": "7.1.0"
}
